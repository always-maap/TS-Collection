{
  "name": "@ts-collection/core",
  "version": "0.0.3",
  "description": "collection of algorithm and data structure for typescript",
  "main": "dist/cjs/bundle.cjs.js",
  "module": "dist/esm/bundle.esm.js",
  "sideEffects": false,
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build-clean": "yarn clean && yarn build",
    "build:types": "tsc --emitDeclarationOnly",
    "build:bundle": "rollup -c",
    "build": "npm run build:types && npm run build:bundle",
    "dev": "nodemon src/index.ts",
    "test": "jest",
    "lint": "eslint src",
    "prepare": "husky install"
  },
  "repository": "git+https://github.com/always-maap/TS-Collection.git",
  "keywords": [
    "typescript",
    "algorithm",
    "data structure",
    "collections-typescript"
  ],
  "author": "tur1ng <mohammadali.ap.2000@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/always-maap/TS-Collection/issues"
  },
  "homepage": "https://github.com/always-maap/TS-Collection#readme",
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-typescript": "7.15.0",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "19.0.2",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@trivago/prettier-plugin-sort-imports": "3.1.1",
    "@types/jest": "26.0.24",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "4.27.0",
    "@typescript-eslint/parser": "4.27.0",
    "all-contributors-cli": "6.20.0",
    "babel-jest": "26.6.3",
    "babel-plugin-transform-class-properties": "6.24.1",
    "husky": "7.0.4",
    "jest": "26.6.3",
    "nodemon": "2.0.13",
    "rollup": "2.59.0",
    "rollup-plugin-size": "0.2.2",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-visualizer": "5.5.0",
    "ts-node": "9.0.0",
    "typescript": "4.4.4"
  }
}
